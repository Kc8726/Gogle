class SearchEngine:
    def __init__(self):
        self.documents = {}

    def add_document(self, doc_id, content):
        self.documents[doc_id] = content

    def search(self, query):
        results = []
        for doc_id, content in self.documents.


if __name__ == "__main__":
    search_engine = SearchEngine()

    # Добавление документов
    search_engine.add_document(1, "Это пример текста для поисковой системы.")
    search_engine.    

            )

    # Выполнение поиска
    query = "поиск Python"
    search_results = search_engine.search(query)

    if search_results:
        print(f"Результаты по запросу '{query}':")
        for doc_id in search_results:
            print(f"Документ {doc_id}: {search_engine.documents[doc_id]}")
    else:
        print("Ничего не найдено.")
